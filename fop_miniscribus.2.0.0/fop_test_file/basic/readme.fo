&amp;lt;fo:block&amp;gt;text&amp;lt;/fo:block&amp;gt;</fo:block>
   <fo:block margin-bottom="1pt" id="blocknr_183" color="rgb(0,0,0,255)" >
&amp;lt;/fo:table-cell&amp;gt;</fo:block>
   <fo:block margin-bottom="1pt" id="blocknr_184" color="rgb(0,0,0,255)" >
&amp;lt;fo:table-cell&amp;gt;</fo:block>
   <fo:block margin-bottom="1pt" id="blocknr_185" color="rgb(0,0,0,255)" >
&amp;lt;fo:block&amp;gt;text&amp;lt;/fo:block&amp;gt;</fo:block>
   <fo:block margin-bottom="1pt" id="blocknr_186" color="rgb(0,0,0,255)" >
&amp;lt;/fo:table-cell&amp;gt;</fo:block>
   <fo:block margin-bottom="1pt" id="blocknr_187" color="rgb(0,0,0,255)" >
